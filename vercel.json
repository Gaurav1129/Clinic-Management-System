{
    "version": 2,
    "routes": [
      {
        "src": "/api/doctors",
        "dest": "/api/doctors.js"
      },
      {
        "src": "/api/doctors/:id",
        "dest": "/api/doctors/[id].js"
      },
      {
        "src": "/api/doctors/:id/availability",
        "dest": "/api/doctors/availability.js"
      },
      {
        "src": "/api/appointments/book",
        "dest": "/api/appointments/book.js"
      }
    ]
  }
  